A,CAAA,OAAA,GAAA,OAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,UAAA,GAAA,OAAA,CAAA,UAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,OAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,OAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,UAAA,GAAA,OAAA,CAAA,UAAA;AACA,OAXA;AAYA,KAtDA;AAuDA;AACA,IAAA,WAxDA,uBAwDA,GAxDA,EAwDA;AACA,WAAA,QAAA,CAAA,UAAA,GAAA,GAAA;AACA,KA1DA;AA2DA;AACA,IAAA,cA5DA,0BA4DA,GA5DA,EA4DA;AACA,WAAA,QAAA,CAAA,OAAA,GAAA,GAAA;AACA;AA9DA;AA7CA,CAAA","sourcesContent":["<template>\n  <div class=\"content_box\">\n    <first-title\n      :title=\"parentId === 0 ? '新增(学院/企业)' : '编辑(学院/企业)'\"\n    />\n    <div class=\"bg-white\" style=\"padding: 50px 20px\">\n      <el-form ref=\"form\" :rules=\"rules\" label-width=\"180px\" :model=\"formdata\">\n        <el-form-item label=\"学院/企业名称：\" prop=\"title\">\n          <el-input\n            style=\"width: 250px\"\n            v-model=\"formdata.title\"\n            placeholder=\"请输入学院/企业名称\"\n          />\n        </el-form-item>\n        <el-form-item label=\"学院/企业主要方向：\" prop=\"title\">\n          <el-input\n            style=\"width: 350px\"\n            v-model=\"formdata.label\"\n            placeholder=\"请输入学院/企业主要方向(多个以/隔开)\"\n          />\n        </el-form-item>\n        <el-form-item\n          class=\"float_item\"\n          style=\"height: auto\"\n          label=\"列表小图:\"\n          prop=\"coverImage\"\n        >\n          <upload-img\n            width=\"80px\"\n            height=\"80px\"\n            :img=\"formdata.coverImage\"\n            @uploadSuc=\"uploadSucFn\"\n          />\n        </el-form-item>\n        <el-form-item\n          class=\"float_item\"\n          style=\"height: auto\"\n          label=\"详情大图:\"\n          prop=\"coverImage\"\n        >\n          <upload-img\n            width=\"80px\"\n            height=\"80px\"\n            :img=\"formdata.inImage\"\n            @uploadSuc=\"uploadSucFnbig\"\n          />\n        </el-form-item>\n        <el-form-item label=\"添加类型(学院/企业)：\" prop=\"type\">\n          <el-select v-model=\"formdata.type\" placeholder=\"请选择添加类型\">\n            <el-option :label=\"'学院'\" :value=\"1\"></el-option>\n            <el-option :label=\"'企业'\" :value=\"2\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"观看次数：\" prop=\"watchedNum\">\n          <el-input\n            min=\"0\"\n            type=\"number\"\n            style=\"width: 250px\"\n            v-model=\"formdata.watchedNum\"\n            placeholder=\"请输入观看次数\"\n          />\n        </el-form-item>\n        <el-form-item label=\"排序(越小越前)：\" prop=\"sort\">\n          <el-input\n            min=\"0\"\n            type=\"number\"\n            style=\"width: 250px\"\n            v-model=\"formdata.sort\"\n            placeholder=\"请输入排序数\"\n          />\n        </el-form-item>\n        <el-form-item style=\"clear: both\" label=\"学院/企业就业学员信息：\">\n          <el-table\n            class=\"list_table\"\n            :data=\"formdata.workInfo\"\n            border\n            style=\"width: 800px\"\n          >\n            <el-table-column\n              label=\"就业类型名称(例如:数控)\"\n              width=\"250\"\n              align=\"center\"\n            >\n              <template slot-scope=\"scope\">\n                <el-input\n                  v-model=\"scope.row.name\"\n                  class=\"spec_name\"\n                  placeholder=\"填写就业类型名称\"\n                />\n              </template>\n            </el-table-column>\n            <el-table-column label=\"就业数量\" width=\"200\" align=\"center\">\n              <template slot-scope=\"scope\">\n                <el-input\n                  type=\"number\"\n                  v-model=\"scope.row.num\"\n                  class=\"spec_name\"\n                  placeholder=\"填写就业数量\"\n                />\n              </template>\n            </el-table-column>\n            <el-table-column\n              label=\"操作\"\n              align=\"center\"\n              header-align=\"center\"\n              min-width=\"300\"\n            >\n              <template slot-scope=\"scope\">\n                <el-button\n                  size=\"small\"\n                  type=\"danger\"\n                  @click=\"delListFn(scope.$index)\"\n                  >删除</el-button\n                >\n              </template>\n            </el-table-column>\n          </el-table>\n          <el-button\n            size=\"small\"\n            icon=\"el-icon-plus\"\n            style=\"margin-top: 15px\"\n            type=\"primary\"\n            @click=\"addListItemFn\"\n            >添加就业类型信息</el-button\n          >\n        </el-form-item>\n        <el-form-item style=\"clear: both\" label=\"描述：\" prop=\"content\">\n          <tinymce\n            :width=\"650\"\n            ref=\"ppp\"\n            :id=\"'parentdetail_' + parentId\"\n            v-model=\"formdata.content\"\n            :height=\"400\"\n          />\n        </el-form-item>\n        <el-form-item label=\"\">\n          <el-button\n            type=\"primary\"\n            class=\"mr10\"\n            plain\n            @click=\"$router.push({ name: 'editPartnerlInfo' })\"\n            >取消</el-button\n          >\n          <el-button type=\"primary\" :disabled=\"submitIng\" @click=\"submit\">{{\n            submitIng ? \"提交中...\" : \"确认提交\"\n          }}</el-button>\n          <span v-show=\"submitIng\" style=\"margin-left: 15px; color: #f56c6c\"\n            >提交中,请勿重复点击!</span\n          >\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { editPartnerlInfo, getPartnerlInfo } from \"@/api/talent.js\";\nimport uploadImg from \"@/components/uploadImg/upload-img.vue\";\nimport Tinymce from \"@/components/Tinymce\";\nexport default {\n  name: \"\",\n  components: {\n    Tinymce,\n    uploadImg,\n  },\n  props: {},\n  data() {\n    return {\n      parentId: \"\",\n      submitIng: false,\n      typeList: [\n        { type: 0, name: \"学院\" },\n        { type: 1, name: \"企业\" },\n      ],\n      formdata: {\n        label: \"\",\n        inImage: \"\",\n        content: \"\",\n        coverImage: \"\",\n        sort: \"\",\n        title: \"\",\n        type: \"\",\n        watchedNum: \"\",\n        workInfo: [],\n      },\n      rules: {},\n    };\n  },\n  mounted() {\n    let aaa = this.$route.query.parentId;\n    this.parentId = Number(aaa);\n    if (Number(aaa) !== 0) {\n      this.getPartnerDetail();\n    }\n  },\n  created() {},\n  activated() {},\n  watch: {\n    parentId(newval) {\n      if (newval !== 0) {\n        this.$refs.ppp.setContent(\"\");\n      }\n    },\n  },\n  methods: {\n    //添加就业信息\n    addListItemFn() {\n      this.formdata.workInfo.push({ name: \"\", num: \"\" });\n    },\n    //删除就业学员信息\n    delListFn(index) {\n      this.formdata.workInfo.splice(index, 1);\n    },\n    //submit\n    submit() {\n      // return console.log(this.formdata);\n      const postData = { ...this.formdata };\n      postData.workInfo = JSON.stringify(postData.workInfo);\n      postData.id = this.parentId;\n      postData.sort = Number(postData.sort);\n      postData.type = Number(postData.type);\n      postData.watchedNum = Number(postData.watchedNum);\n      this.submitIng = true;\n      editPartnerlInfo(postData)\n        .then((res) => {\n          setTimeout(() => {\n            this.submitIng = false;\n          }, 1000);\n          if (res.code == \"000\") {\n            this.$message({\n              showClose: true,\n              message: this.parentId === 0 ? \"新增成功！\" : \"编辑成功！\",\n              type: \"success\",\n            });\n            this.$router.go(-1);\n          }\n        })\n        .catch(() => {\n          setTimeout(() => {\n            this.submitIng = false;\n          }, 1000);\n        });\n    },\n    //获取详情\n    getPartnerDetail() {\n      if (this.parentId == 0) return;\n      getPartnerlInfo(this.parentId).then((res) => {\n        const resData = this.resDataFn(res);\n        this.formdata.workInfo = resData.list;\n        this.formdata.content = resData.content;\n        this.formdata.inImage = resData.inImage;\n        this.formdata.label = resData.label;\n        this.formdata.coverImage = resData.coverImage;\n        this.formdata.sort = resData.sort;\n        this.formdata.title = resData.title;\n        this.formdata.type = resData.type;\n        this.formdata.watchedNum = resData.watchedNum;\n      });\n    },\n    //图片上传返回\n    uploadSucFn(img) {\n      this.formdata.coverImage = img;\n    },\n    //图片上传返回详情大图\n    uploadSucFnbig(img) {\n      this.formdata.inImage = img;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n</style>\n"],"sourceRoot":"src/views/talent/editPartnerlInfo"}]}