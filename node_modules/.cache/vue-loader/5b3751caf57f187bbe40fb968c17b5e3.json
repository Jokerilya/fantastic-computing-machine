t\"\n            :props=\"{\n              children: 'list',\n              label: 'name',\n              value: 'id',\n            }\"\n            @change=\"typeChange\"\n          ></el-cascader>\n        </div>\n      </div>\n      <div class=\"custorCode\">\n        <div class=\"code\">设备产地</div>\n        <div class=\"codeInput\">\n          <el-select\n            v-model=\"place\"\n            placeholder=\"选择产地\"\n            @change=\"placeChange\"\n          >\n            <el-option label=\"国产\" value=\"国产\"></el-option>\n            <el-option label=\"进口\" value=\"进口\"></el-option>\n          </el-select>\n        </div>\n      </div>\n    </div>\n    <div class=\"bottomLine\" v-if=\"activeName == 'second'\">\n      <div class=\"custorName\">\n        <div class=\"name\">维保时间</div>\n        <div class=\"nameInput\">\n          <el-date-picker\n            v-model=\"choseeTime\"\n            type=\"daterange\"\n            range-separator=\"至\"\n            start-placeholder=\"开始日期\"\n            end-placeholder=\"结束日期\"\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\n            @change=\"timeChange\"\n          ></el-date-picker>\n        </div>\n      </div>\n    </div>\n    <div class=\"bottomLine2\" v-if=\"activeName == 'second'\">\n      <div class=\"custorCode\">\n        <div class=\"code\" style=\"margin:10px 0\">铭牌图片</div>\n        <div class=\"codeInput\">\n          <el-upload\n            action\n            list-type=\"picture-card\"\n            :on-preview=\"handlePictureCardPreview\"\n            :on-remove=\"handleRemove\"\n            :on-success=\"uploadSuccess\"\n            :http-request=\"httpRequestFn\"\n          >\n            <i class=\"el-icon-plus\"></i>\n          </el-upload>\n        </div>\n        <div\n          class=\"btnSmbit\"\n          v-if=\"activeName == 'second'\"\n          style=\"margin:50px 0\"\n        >\n          <el-button\n            v-if=\"!change\"\n            type=\"success\"\n            size\n            plain\n            @click=\"_editButlerOrder\"\n            >保存基本信息</el-button\n          >\n          <el-button\n            v-if=\"change\"\n            type=\"success\"\n            size\n            plain\n            @click=\"_changeForm\"\n            >确认修改</el-button\n          >\n        </div>\n      </div>\n    </div>\n\n    <el-dialog :visible.sync=\"dialogVisible\">\n      <img width=\"100%\" :src=\"param.nameplateImg\" alt />\n    </el-dialog>\n\n    <div class=\"titlePart\" v-if=\"activeName == 'second'\">\n      <div class=\"titleOne\">设备配置信息</div>\n      <el-button icon=\"el-icon-refresh\" plain type=\"primary\" @click=\"addPart\"\n        >新增</el-button\n      >\n    </div>\n    <div class=\"listPart\" v-if=\"activeName == 'second'\">\n      <el-table\n        highlight-current-row\n        :data=\"deviceList.partsList\"\n        style=\"width: 100%;\"\n      >\n        <el-table-column\n          prop=\"category\"\n          label=\"项目大类\"\n          show-overflow-tooltip\n          width=\"200\"\n          align=\"center\"\n        ></el-table-column>\n        <el-table-column\n          prop=\"name\"\n          label=\"项目名称\"\n          show-overflow-tooltip\n          width=\"200\"\n          align=\"center\"\n        ></el-table-column>\n        <el-table-column\n          prop=\"deviceBrand\"\n          label=\"设备品牌\"\n          show-overflow-tooltip\n          width=\"200\"\n          align=\"center\"\n        ></el-table-column>\n        <el-table-column\n          prop=\"deviceModel\"\n          label=\"型号\"\n          show-overflow-tooltip\n          width=\"150\"\n          align=\"center\"\n        ></el-table-column>\n        <el-table-column\n          prop=\"unit\"\n          label=\"单位\"\n          show-overflow-tooltip\n          width=\"150\"\n          align=\"center\"\n        ></el-table-column>\n        <el-table-column\n          prop=\"specification\"\n          label=\"规格\"\n          show-overflow-tooltip\n    