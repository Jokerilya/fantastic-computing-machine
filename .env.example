# Example environment variables for development
# Copy this file to .env and fill values. Do NOT commit real secrets.

# Base API for axios requests
VUE_APP_BASE_API=http://localhost:3000

# AES key (hex string) used by front-end encryption (if enabled on server).
# Example: d86d7bab3d6ac01ad9dc6a897652f2d2
VUE_APP_AES_KEY=

# AMap (高德) API key for vue-amap
VUE_APP_AMAP_KEY=

# Build metadata (optional override)
# VUE_APP_BUILD_VERSION=1
# VUE_APP_BUILD_TIME=2025-01-01T00:00:00.000Z
